<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sun.DepartmentEntity" table="department" schema="meetingroom">
        <id name="departmentId">
            <column name="department_id" sql-type="int(11)"/>
        </id>
        <property name="departmentName">
            <column name="department_name" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="departmentNum">
            <column name="department_num" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>