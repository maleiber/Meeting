<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sun.DepartmentRelationStaffEntity" table="department_relation_staff" schema="meetingroom">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="staffId">
            <column name="staff_id" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="departmentId">
            <column name="department_id" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>