<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.sun.AuditEntity" table="audit" schema="meetingroom">
        <id name="auditId">
            <column name="audit_id" sql-type="int(11)"/>
        </id>
        <property name="auditName">
            <column name="audit_name" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="auditTele">
            <column name="audit_tele" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="auditEmail">
            <column name="audit_email" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>